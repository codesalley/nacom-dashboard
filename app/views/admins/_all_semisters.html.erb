<% semisters.each do |item| %>

<div class="main-card d-flex shadow-sm p-3" >
    <div class="d-img">
        <%= image_tag('demo.png') %>
    </div>
    <div class="detail m-0">
        <p class="m-0"><%= item.name %></p>
        <small>Started date:  <%= item.date %></small>
    </div>
</div>
    
<% end %>