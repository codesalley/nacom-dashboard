<div class="form-group">
  <%= form_with model: student, url: add_student_path do |f|%>
  <div class="form-group mb-3">
    <%= f.label :first_name %> <%= f.text_field :first_name %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :middle_name %> <%= f.text_field :middle_name %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :last_name %> <%= f.text_field :last_name %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :index_number %> <%= f.text_field :index_number %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :password %> <%= f.password_field :password %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :address %> <%= f.text_field :address %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :course %> <%= f.text_field :course %>
  </div>
  <div class="form-group mb-3">
    <%= f.label :enroll_year %> <%= f.datetime_field :enroll_year %>
  </div>
  <div class="form-group mb-3">
    <%= f.submit "Submit", disable_with: 'Submiting...' %>
  </div>
  <% end %>
</div>
