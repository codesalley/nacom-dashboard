<%= form_with model: result, url: add_results_path do |f| %>

<div class="form-group mb-3">
  <%= f.label :course_name, "course_name" %> <%= f.text_field :course_name %>
</div>

<div class="form-group mb-3">
  <%= f.label :Semister, "Semister" %> <%= f.select('Semister
  name',@semister_options, {} ) %>
</div>
<div class="form-group mb-3">
  <%= f.label :Student, "Student" %> <%= f.select('Student_id',
  @students_options, {} ) %>
</div>
<div class="form-group mb-3">
  <%= f.submit "Submit", disable_with: 'Submiting...' %>
</div>

<% end %>
