<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-center" id="staticBackdropLabel">Add Semister</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">

        
<div class="form-group">
  <%= form_with model: semister, url: add_semister_path do |f| %>
  <div class="form-floating mb-3">
    <%= f.text_field :name,  class: 'form-control', id:"floatingInput" %>
    <%= f.label :semister_name,for:"floatingInput" %>
  </div>
  <div class="form-group mb-3 d-flex flex-column form-floating">
    <%= f.date_field :date,  class:"form-control", id:"floatingInput"%>
    <%= f.label :date ,for:"floatingInput" %> 
  </div>
  <div class="form-group mb-3">
    
    <%= f.submit "Submit", disable_with: 'Submiting...', class: 'btn btn-outline-success w-100 p-2' %>
  </div>
  <% end %>
</div>
      </div>

    </div>
  </div>
</div>


